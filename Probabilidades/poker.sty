 \or%
   \renewcommand{\pkr@val}{#1}%
   \renewcommand{\pkr@drawsymbols}{\pkr@sevsymbols}%
 \or%
   \renewcommand{\pkr@val}{#1}%
   \renewcommand{\pkr@drawsymbols}{\pkr@sixsymbols}%
 \or%
   \renewcommand{\pkr@val}{#1}%
   \renewcommand{\pkr@drawsymbols}{\pkr@fivesymbols}%
