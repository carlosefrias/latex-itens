    \renewcommand{\colors}{\color{pkrblack}}%
    \renewcommand{\colorh}{\color{pkrgray}}%
    \renewcommand{\colord}{\color{pkrgray}}%
    \renewcommand{\colorc}{\color{pkrblack}}%
    \renewcommand{\pkr@facecol}{color}%
